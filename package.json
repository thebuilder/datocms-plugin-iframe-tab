    "datocms-plugins-sdk": "^0.1.2",
    "datocms-react-ui": "^0.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "prettier": "^2.4.1",
    "vite": "^2.6.3"
